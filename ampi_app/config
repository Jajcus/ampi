[UI]
width=792
height=464

[Jack]
wait_for_device=true
device=hw:1
rate=48000
frames=128
periods=3
io_latency_in=445
io_latency_out=445
cmdline=/usr/bin/jackd -dalsa -d${device} -r${rate} -p${frames} -n${periods} -I${io_latency_in} -O${io_latency_out}

[Guitarix]
rpc_host=127.0.0.1
rpc_port=9090
cmdline=/usr/bin/guitarix --rpchost=${rpc_host} --rpcport=${rpc_port}
safe=Ampi,empty
default=Ampi,clean

[System]
Shutdown=/bin/systemctl poweroff
Reboot=/bin/systemctl reboot

# vi: ft=desktop
